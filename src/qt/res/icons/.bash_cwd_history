ls -h
ls --help
ls | grep .png
echo $(ls | grep .png)
for f in $(ls | grep .png); do  convert $f  -modulate 100,0  $f; done
ping 2a02:4780:1:1::1:21f4
ping -h
ping6 2a02:4780:1:1::1:21f4
ping6 ipv6.google.com
[ -f /proc/net/if_inet6 ] && echo 'IPv6 ready system!' || echo 'No IPv6 support found! Compile the kernel!!'
